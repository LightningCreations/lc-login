# Root Options



srcdir = @srcdir@
builddir = @builddir@
abs_builddir = @abs_builddir@

RUSTC = @RUSTC@
RUSTFLAGS := @RUSTFLAGS@

INSTALL := @INSTALL@

RUSTC_IS_LCCC = @rustc_is_lccc@

## Install directories

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
includedir = @includedir@
sbindir = @sbindir@
libexecdir = @libexecdir@
datarootdir = @datarootdir@
datadir = @datadir@
mandir = @mandir@
docdir = @docdir@
infodir = @infodir@
localedir = @localedir@
sysconfdir = @sysconfdir@
localstatedir = @localstatedir@
runstatedir = @runstatedir@
sharedstatedir = @sharedstatedir@
usersdir = @usersdir@
groupsdir = @groupsdir@
passwd = @passwd@
shadow = @shadow@
group = @group@
gshadow = @gshadow@
sudoers = @sudoers@

# Other Configuration

features = @features@
binaries = @binaries@

# Package specific Configuration

OUTPUT = liblc-login.rlib

CRATE_NAME = lc_login

DEPENDENCIES = lazy_static install-dirs libc fake-enum openssl bytemuck zeroize defer itertools rpassword unshare


# Autogenerated variables

binary_outputs = $(binaries:%=%@EXEEXT@)

CFG = $(foreach feature,$(features),feature="$feature") @detected_cfg@

SUBDIRS = vendor

EXTERN_CRATES = $(foreach dep,$(DEPENDENCIES),$(abs_bindir)/vendor/$(dep)/lib$(dep).rlib)

# Targets

all: stamp

.PHONY: all clean distclean install install-strip vendor/clean vendor/distclean vendor/install vendor/install-strip

Makefile: config.status @srcdir@/Makefile.in
    @builddir@/config.status Makefile

config.status: @srcdir@/configure
    @builddir@/config.status --recheck


stamp: $(binaries) liblc-login.rlib
    touch stamp

liblc-login.rlib: Makefile vendor/stamp

vendor/stamp: vendor/Makefile
    make -C vendor stamp

vendor/Makefile: vendor/config.status
    cd @builddir@/vendor && ./config.status Makefile

vendor/config.status: @srcdir@/vendor/configure
    cd @builddir@/vendor && ./config.status --rechecke

vendor/clean:
    make -C vendor clean

vendor/distclean:
    make -C vendor distclean

vendor/install:
    make -C vendor install

vendor/install-strip:
    make -C vendor install-strip 

include liblc-login.rlib.d

$(OUTPUT).d: src/lib.rs vendor/stamp
    $(RUSTC) $(RUSTFLAGS) $(foreach cfg,$(CFG),--cfg $(cfg)) --crate-name $(CRATE_NAME) --crate-type rlib --emit dep-info=@builddir@/$(OUTPUT).d -o$(OUTPUT) $(EXTERN_CRATES)

liblc-login.rlib: src/lib.rs
    $(RUSTC) $(RUSTFLAGS) $(foreach cfg,$(CFG),--cfg $(cfg)) --crate-name lc_login --crate-type rlib --emit dep-info=@builddir@/$(OUTPUT).d --emit link=@abs_builddir@/$(OUTPUT) -o$(OUTPUT) $(EXTERN_CRATES)

$(binary_outputs): %@EXEEXT@:

